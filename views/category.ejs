<%- include('partials/head'); %>

<body>
    <div class="flexboxCenter bg-green">
        <header>
            <h1>BuckStar</h1>
            <%- include('partials/menu'); %>
        </header>
    </div>

    <div class="flexboxCenter">
        <article>
            <div id="catTableCont">
                <table id="catTable">
                    <caption>Kategorier</caption>
                    <tbody>
                        <tr>
                            <td>Kategori (ID)</td>
                        </tr>
                        <% for (const row of res) { %>
                        <tr>
                            <td><%= row.kategori_typ %> (<%= row.id %>)</td>
                        </tr>
                        <% }; %>
                    </tbody>
                </table>
            </div>
        </article>
    </div>
    <div class="footerCont">
        <%- include('partials/footer') %>
    </div>
</body>